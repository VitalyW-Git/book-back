{
  "name": "book-back",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node dist/bin/www.js",
    "dev": "nodemon --exec ts-node bin/www.ts",
    "dev:watch": "nodemon --watch . --ext ts --exec ts-node bin/www.ts",
    "format": "prettier --write \"**/*.{ts,json}\"",
    "type-check": "tsc --noEmit",
    "lint": "npm run format && npm run type-check"
  },
  "dependencies": {
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.6",
    "@types/http-errors": "^2.0.5",
    "@types/node": "^24.10.1",
    "nodemon": "^2.0.22",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
